{
  "name": "semantic-operating-model",
  "version": "0.1.0",
  "description": "Tier-0 Semantic Operating Model for NSW",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsx watch src/server.ts --max-old-space-size=800",
    "build": "tsc",
    "start": "node --max-old-space-size=800 dist/src/server.js",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.{ts,json}\"",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "semantic-model",
    "event-sourcing",
    "graph-database"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
    "@hono/node-server": "^1.19.7",
    "@som/shared-types": "*",
    "@types/pg": "^8.16.0",
    "better-sqlite3": "^12.5.0",
    "bullmq": "^5.29.0",
    "dotenv": "^17.2.3",
    "hono": "^4.10.7",
    "ioredis": "^5.8.2",
    "json-logic-js": "^2.0.5",
    "neo4j-driver": "^6.0.1",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "tsx": "^4.21.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/json-logic-js": "^2.0.8",
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^4.0.15",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "eslint": "^9.39.1",
    "fast-check": "^3.15.0",
    "globals": "^16.5.0",
    "rollup": "^4.53.3",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.49.0",
    "typescript-json-schema": "^0.67.0",
    "vitest": "^4.0.15"
  }
}
