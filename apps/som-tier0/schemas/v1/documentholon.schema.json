{
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "const": "Document"
    },
    "properties": {
      "type": "object",
      "properties": {
        "referenceNumbers": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "title": {
          "type": "string"
        },
        "documentType": {
          "type": "string",
          "enum": [
            "Policy",
            "Order",
            "Plan",
            "SOP",
            "Record",
            "Instruction",
            "Manual",
            "Charter",
            "Framework",
            "CONOPS",
            "OPLAN",
            "EXORD"
          ]
        },
        "version": {
          "type": "string"
        },
        "effectiveDates": {
          "type": "object",
          "properties": {
            "start": {
              "type": "string",
              "format": "date-time"
            },
            "end": {
              "type": "string",
              "format": "date-time"
            }
          },
          "additionalProperties": false,
          "required": [
            "start"
          ]
        },
        "classificationMetadata": {
          "type": "string"
        },
        "content": {
          "type": "string"
        },
        "supersedes": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "derivedFrom": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "additionalProperties": false,
      "required": [
        "classificationMetadata",
        "documentType",
        "effectiveDates",
        "referenceNumbers",
        "title",
        "version"
      ]
    },
    "id": {
      "description": "Core holon type definitions for the Semantic Operating Model",
      "type": "string"
    },
    "createdAt": {
      "type": "string",
      "format": "date-time"
    },
    "createdBy": {
      "type": "string"
    },
    "status": {
      "enum": [
        "active",
        "inactive"
      ],
      "type": "string"
    },
    "sourceDocuments": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "createdAt",
    "createdBy",
    "id",
    "properties",
    "sourceDocuments",
    "status",
    "type"
  ],
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://som.tier0/schemas/v1/documentholon.schema.json"
}